{
  "name": "zoobc",
  "version": "0.0.4-pre-release",
  "description": "A small set of libraries to implement applications connect with the P2P API of the nodes in the blockchain",
  "author": "ZooBC SDK Team",
  "license": "Apache-2.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "keywords": [
    "blockchain",
    "blockchain-zoo",
    "zoobc",
    "zoobc-sdk-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoobc/zoobc-sdk"
  },
  "bugs": {
    "url": "https://github.com/zoobc/zoobc-sdk/issues"
  },
  "scripts": {
    "start": "tsc-watch --onCompilationComplete",
    "clean": "rimraf dist",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "build": "run-s clean && npm run prettier && tsc -p .",
    "lint": "eslint \"{src,tests}/**/*\" --fix",
    "prettier": "prettier --write \"{src,tests}/**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/*.{ts,test.ts}": [
      "npm run prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.9.6",
    "bip32": "https://github.com/BlockchainZoo/bip32#SLIP10",
    "bip39": "^3.0.2",
    "bn.js": "^5.1.1",
    "buffer": "^5.5.0",
    "crypto-js": "^4.0.0",
    "grpc-web": "^1.0.5",
    "rxjs": "^6.5.4",
    "stream": "^0.0.2",
    "string_decoder": "^1.3.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@improbable-eng/grpc-web-node-http-transport": "^0.9.6",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.1.7",
    "@types/crypto-js": "^3.1.43",
    "@types/google-protobuf": "^3.7.2",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "install": "^0.13.0",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.18",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.6.2",
    "ts-protoc-gen": "^0.12.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "google-protobuf": "^3.9.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6.0.0"
  }
}