{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.renderWhitespace": "all",
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "eslint.codeActionsOnSave": true,
  // "eslint.validate": [
  //   "javascript",
  //   {
  //     "language": "typescript",
  //     "autoFix": true
  //   }
  // ],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": true
  }
}